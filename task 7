def move(subject, x1, x2):
    return f"Move {subject} from {x1} to {x2}"

def push_box(x1, x2):
    return f"Push box from {x1} to {x2}"

def climb_box(x, direction):
    return f"Climb box at {x} {direction}"

def have_banana(x):
    return f"Have banana at {x}"

# Initial State
initial_state = {
    'monkeyAt0': True,
    'monkeyLevel': 'Down',
    'bananaAt1': True,
    'boxAt2': True
}
goal_state = {
    'GetBanana': True,
    'at': 1
}
def plan_actions(initial_state, goal_state):
    actions = []

    monkey_pos = 0 if initial_state.get('monkeyAt0', False) else None
    banana_pos = 1 if initial_state.get('bananaAt1', False) else None
    box_pos = 2 if initial_state.get('boxAt2', False) else None
    if monkey_pos != box_pos:
        actions.append(move('Monkey', monkey_pos, box_pos))
        monkey_pos = box_pos
    if box_pos != banana_pos:
        actions.append(push_box(box_pos, banana_pos))
        box_pos = banana_pos
    actions.append(climb_box(box_pos, 'Up'))
    actions.append(have_banana(banana_pos))

    return actions

actions = plan_actions(initial_state, goal_state)

print("Plan:")
for action in actions:
    print(action)
